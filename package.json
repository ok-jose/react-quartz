{
  "name": "react-quartz",
  "version": "1.0.5",
  "description": "Antd Quartz Cron Component - React",
  "main": "src/lib/index.ts",
  "module": "dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ok-jose/react-quartz.git"
  },
  "keywords": [
    "react",
    "cron",
    "quartz",
    "antd",
    "ant design",
    "cron generator",
    "quartz cron"
  ],
  "scripts": {
    "dev": "vite",
    "copy-package": "cp package.json dist",
    "build": "tsc && pnpm copy-package && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "clean-pkgs": "pnpm -r exec rm -rf dist .turbo .swc *.log node_modules",
    "clean": "pnpm clean-pkgs && rm -rf node_modules",
    "build-storybook": "storybook build",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "publish": "npm publish ./dist"
  },
  "dependencies": {
    "@sbzen/cron-core": "^2.0.2"
  },
  "peerDependencies": {
    "antd": "^5.15.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.23",
    "@storybook/addon-essentials": "^8.0.2",
    "@storybook/addon-interactions": "^8.0.2",
    "@storybook/addon-links": "^8.0.2",
    "@storybook/addon-onboarding": "^8.0.2",
    "@storybook/blocks": "^8.0.2",
    "@storybook/react": "^8.0.2",
    "@storybook/react-vite": "^8.0.2",
    "@storybook/test": "^8.0.2",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "storybook": "^8.0.2",
    "typescript": "^5.2.2",
    "prettier": "^3.2.5",
    "vite": "^5.1.6"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
